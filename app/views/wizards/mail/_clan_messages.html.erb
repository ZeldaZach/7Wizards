<div id="columns_narrow_wide">
  <div id="col_left">
    <%= render :partial => "menu" %>
  </div>

  <div id="col_right" >
     <div class="main_col_content">
    <div id="messages_list" class="quick_links">
     <h1 class="category_title_shop"><%= t(:title) %></h1>
      <div class="bes_search">
        <table class="content_up table" cellpadding="0" cellspacing="0">

          <tr class="tableWidth">
            <th scope="col" class="index_cell">#</th>
            <th scope="col" ></th>
            <th scope="col" style="width: 80px"><%= tg(:date_title) %></th>
          </tr>
          <% @history.each_with_index do |msg, index|%>

            <%if !msg.title.nil?%>
              <tr>
                <td class="index_cell"><%= paginate_index(@page, index) %></td>
                <% if msg.message %>
                  <td><%= msg.g_message(self) %></td>
                <%else%>
                  <td><%= msg.g_title(self) %></td>
                <%end%>
                <td><%= d msg.created_at %></td>
              </tr>
            <%end%>
          <%end%>
          
        </table>
      </div>
    </div>
       <div class="clear"></div>
       <%=paginate(@history)%>
<div class="clear"></div>
     <div class="main_col_bg"></div>
      <div class="main_col_border_bottom"></div>
      <div class="main_col_border_top"></div>
  </div>
  </div>
  <div class="clear"></div>
</div>
